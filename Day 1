class classcreation {
    int rollNumber;
    String name;

classcreation(int i, String n){
    rollNumber = i;
    name = n;
}

    
}

public class Main {
    public static void main(String[] args) {
        classcreation obj = new classcreation(1,"Nikhil");
        obj.rollNumber = 1;
        obj.name = "Nikhil";

        System.out.println(obj.rollNumber);
        System.out.println(obj.name);
    }
}
