#code1
// Inheritance
// Single Inheritance 
class Vehicle{
    void start(){
        System.out.println("Vehicle Start");
    }
    
}
class Bike extends Vehicle { // child class
void drive(){
    System.out.println("Bike Running");
}
    
}
class Main {
    public static void main(String[] args) {
        Bike b = new Bike();
        b.start();
        b.drive();
       
    }
}
