#Code 1
class Student{
    static String collegeName="IILM";
    String name;
    
    Student(String name ){
        this.name= name;
        
    }
    void display(){
        System.out.println(name +" Studied at "+ collegeName);
    }
}
class Main {
    public static void main(String[] args) {
        Student s1= new Student("Nikhil");
        // we can chnage the instance variable but can't change the static variable 
        Student s2= new Student("Parshant");
        s1.display();
        s2.display();
    }
}
#code 2
//static method
class Square{
   static int square(int num){
       return num *num;
       
   }
}


class Main {
    public static void main(String[] args) {
        // it is called using the class name 
        System.out.println(Square.square(5));
        
    }
}
// differnce between static and instance variables and methods 
--> Static is accessed by class name
--> instance is accessed by object name
--> in static no need to use this keyword
--> in instance we need to use this keyword 
--> in static we make single copy
--> in instance we make multiple copies

#code3
// Aggrigation And Dependency 
// Address
class Address{ // class
    private String city; // instance
    Address(String city) { // constructor 
        this.city= city;
    }
    public String getCity(){
        return city;
    }
}
// Student class(Aggrigate Addres)
class Student{
   private String name;
   private Address address; // Aggrigation  access modifier , class name , variable name 
   
   Student(String name, Address address){
       this.name = name;
       this.address= address;
       
   }
   void display(){
       System.out.println(name+" lives in " +address.getCity());
   }
    
}

class Main {
    public static void main(String[] args) {
        Address add = new Address("Delhi");
        Student s1 = new Student("Nikhil",add);
        s1.display();
        
       // System.out.println("Try programiz.pro");
    }
}

#code 3
//Dependency 
//one class is dependent on another class temporarily 
// No permanent storage is there 

class Payment{
    void pay(){
        System.out.println("Payment Successful");
    }
}
class Gpay{
    void processPayment(Payment p){ // service provider
        p.pay();
    }
}
class Main {
    public static void main(String[] args) {
        Gpay pay = new Gpay();
        Payment payment = new Payment();
        pay.processPayment(payment);
      //  System.out.println("Try programiz.pro");
    }
}
